.add-button-style {
    width: 50px;
    font-size: 51px;
    background-color: rgba(88, 88, 88, 0);
    border-left-width: 0;
    border-right-width: 0;
    border-top-width: 0;
    border-bottom-width: 0;
    color: rgb(0, 0, 0);
    -unity-text-align: lower-center;
    height: 55px;
    padding-right: 18px;
    background-image: url('/Assets/img/add-button.png');
}

.add-button-style:hover {
    width: 50px;
    font-size: 51px;
    background-color: rgba(88, 88, 88, 0);
    border-left-width: 0;
    border-right-width: 0;
    border-top-width: 0;
    border-bottom-width: 0;
    color: rgb(0, 0, 0);
    -unity-text-align: lower-center;
    height: 55px;
    padding-right: 18px;
    background-image: url('/Assets/img/add-button.png');
    -unity-background-image-tint-color: rgba(255, 255, 255, 0.64);
}
